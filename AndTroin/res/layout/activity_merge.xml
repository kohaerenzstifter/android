<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <LinearLayout android:id="@+id/linearLayout1" android:layout_width="match_parent" android:layout_height="match_parent">
        <LinearLayout android:layout_width="0px" android:orientation="vertical" android:layout_height="match_parent" android:layout_weight="15" android:id="@+id/LinearLayout01">
            <Button android:layout_height="match_parent" android:layout_weight="1" android:layout_width="match_parent" android:text="@string/no" android:enabled="true" android:id="@+id/button_leftno"></Button>
            <Button android:layout_height="match_parent" android:layout_weight="1" android:layout_width="match_parent" android:text="@string/merge" android:enabled="true" android:id="@+id/button_leftmerge"></Button>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearLayout2" android:layout_height="match_parent" android:orientation="vertical" android:layout_width="0px" android:layout_weight="90">
            <org.kohaerenzstiftung.ListView android:layout_height="0px" android:layout_width="match_parent" android:id="@+id/listview_categories1" android:layout_weight="1"></org.kohaerenzstiftung.ListView>
            <org.kohaerenzstiftung.ListView android:layout_height="0px" android:layout_width="match_parent" android:id="@+id/listview_sourcedenominations1" android:layout_weight="1"></org.kohaerenzstiftung.ListView>
            <org.kohaerenzstiftung.ListView android:layout_height="0px" android:layout_width="match_parent" android:id="@+id/listview_targetdenominations1" android:layout_weight="1"></org.kohaerenzstiftung.ListView>

            <org.kohaerenzstiftung.ListView android:layout_height="0px" android:layout_width="match_parent" android:id="@+id/listview_categories2" android:layout_weight="1"></org.kohaerenzstiftung.ListView>
            <org.kohaerenzstiftung.ListView android:layout_height="0px" android:layout_width="match_parent" android:id="@+id/listview_sourcedenominations2" android:layout_weight="1"></org.kohaerenzstiftung.ListView>
            <org.kohaerenzstiftung.ListView android:layout_height="0px" android:layout_width="match_parent" android:id="@+id/listview_targetdenominations2" android:layout_weight="1"></org.kohaerenzstiftung.ListView>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/linearLayout3" android:layout_width="0px" android:layout_height="match_parent" android:layout_weight="15">
            <Button android:layout_height="match_parent" android:layout_weight="1" android:layout_width="match_parent" android:enabled="true" android:id="@+id/button_rightno" android:text="@string/no"></Button>
            <Button android:layout_height="match_parent" android:id="@+id/button_rightmerge" android:text="@string/merge" android:enabled="true" android:layout_width="match_parent" android:layout_weight="1"></Button>
        </LinearLayout>
    </LinearLayout>
    
</LinearLayout>
